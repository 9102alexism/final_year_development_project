{
	"name": "sort-object-value",
	"version": "1.0.3",
	"description": "ðŸŽ€ 0 dependency values based object keys sorting.",
	"main": "build/index.js",
	"files": [
		"build/index.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tylim88/sort-object-value"
	},
	"scripts": {
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm i",
		"test": "jest",
		"coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"prepublishOnly": "npm run build -- --no-demo",
		"build": "webpack --mode production",
		"lint": "eslint --cache --format codeframe --ext mjs,jsx,js src",
		"lint:fix": "npm run lint --fix"
	},
	"keywords": [
		"object",
		"sort",
		"value",
		"ascending",
		"descending"
	],
	"author": "",
	"license": "MIT",
	"module": "src/index.js",
	"devDependencies": {
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@neutrinojs/library": "^9.1.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^25.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-flowtype": "^4.6.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jest": "^23.7.0",
		"eslint-plugin-prettier": "^3.1.2",
		"husky": "^4.2.1",
		"jest": "^25.1.0",
		"neutrino": "^9.1.0",
		"prettier": "^1.19.1",
		"prettier-quick": "0.0.5",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:import/errors",
			"plugin:import/warnings",
			"plugin:prettier/recommended",
			"plugin:flowtype/recommended",
			"plugin:jest/recommended",
			"prettier"
		],
		"rules": {
			"no-unused-vars": [
				"error",
				{
					"args": "after-used"
				}
			]
		},
		"settings": {
			"import/resolver": {
				"node": {
					"moduleDirectory": [
						"node_modules",
						"src/"
					]
				}
			}
		}
	},
	"prettier": {
		"eslintIntegration": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"semi": false,
		"useTabs": true
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	}
}
